<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="birthdayPU" transaction-type="JTA">
        <jta-data-source>jdbc/birthday</jta-data-source>
        <class>org.celebration.celebrationorganization.ejb.user.entity.Privilege</class>
        <class>org.celebration.celebrationorganization.ejb.user.entity.User</class>
        <class>org.celebration.celebrationorganization.ejb.town.entity.Town</class>
    </persistence-unit>
</persistence>
